# Contributor: Rudolf Olah <omouse@gmail.com>
pkgname=gill-sans-font
pkgver=1
pkgrel=1
pkgdesc="Gill Sans font found on Mac OS X, converted by fondu"
url="http://en.wikipedia.org/wiki/Gill_Sans"

depends=()
source=()
md5sums=()


build() {
  cd $startdir
  mkdir -p $startdir/pkg/usr/share/fonts/TTF
  cp $startdir/*.ttf $startdir/pkg/usr/share/fonts/TTF/
}
